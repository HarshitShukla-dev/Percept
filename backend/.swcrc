{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": false,
      "dynamicImport": false
    },
    "transform": {},
    "target": "es2024",
    "loose": false,
    "externalHelpers": false,
    "keepClassNames": false,
    "minify": {
      "compress": {
        "unused": true,
        "dead_code": true,
        "conditionals": true,
        "booleans": true,
        "evaluate": true
      },
      "mangle": true
    }
  },
  "module": {
    "type": "commonjs"
  },
  "minify": true
}
